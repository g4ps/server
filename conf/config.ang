server
{
	# port 64000
	port 8000
	port 80
	location
	{
		path /
		root html/
		autoindex on
		index index.html index.php
		method GET POST
		error_page 404 html/error.html
	}
	location
	{
		path /upload
		root html/upload/
		method GET POST DELETE
		upload on
		autoindex on
		upload_folder html/upload/
	}
}

server
{
	port 8088
	location
	{
		path /
		method GET POST DELETE
		redirect 307 https://google.com
	}
}